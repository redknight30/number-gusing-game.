import random
life=10
print ("hello this the number gusing game you have 10 tryes to guse the coret answer")
roll=random.randint(1,1000)
for count in range(100000**900000):
    num = int(input("what number do you think it is:"))
    if num<roll:
                print ("too low")
                life-=1
    elif num>roll:
                print ("too hight")
                life-=1
    elif num==roll:
                print("you won")
                print (roll)
                break
    if life==0:
                print("game over")
                print (roll)
                break
